.error-wrapper {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 101;
  height: 100vh;
  background-color: #e9e9e9;
  box-sizing: border-box;
  padding: 100rpx 0 0;
}

.item {
  color: #444;
  display: block;
  text-align: center;
}

.code {
  font-size: 120rpx;
  margin: 24rpx 0;
  letter-spacing: 20rpx;
}

.msg {
  font-size: 36rpx;
}