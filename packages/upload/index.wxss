.image-show {
  display: flex;
  flex-wrap: wrap;
}

.show-item {
  position: relative;
  width: 144rpx;
  height: 102rpx;
  padding: 10rpx;
  border-radius: 8rpx;
  box-sizing: border-box;
  margin: 0 38rpx 16rpx 0;
  border: 1rpx dashed #eeeeee;
}

.show-item image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}

.show-addItem {
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #eeeeee;
  background-color: #eeeeee;
}

.icon {
  display: inline-block;
  background-size: contain;
  background-repeat: no-repeat;
}

.icon-abbre {
  width: 62rpx;
  height: 60rpx;
  background-image: url("data:image/png;base64,");
}

.icon-delete {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  z-index: 9;
  width: 24rpx;
  height: 24rpx;
  background-image: url("data:image/png;base64,");
}